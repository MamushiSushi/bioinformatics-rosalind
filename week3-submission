        "####**TASK BP21.2** <font color=orange> **Complete Rosalind #8  Translating RNA into Protein**</font>"
      ],
      "metadata": {
        "id": "9P4_MaXp6O3n"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# rna_to_protein.py\n",
        "# Requires: Biopython\n",
        "from Bio.Seq import Seq\n",
        "# import sys # No longer needed as we are not using sys.argv\n",
        "\n",
        "def main(path=\"rosalind_prot.txt\"):\n",
        "    # Read the RNA string, stripping whitespace/newlines\n",
        "    with open(path, \"r\") as f:\n",
        "        rna = \"\".join(f.read().split())\n",
        "\n",
        "    # Ensure RNA alphabet (just in case the file has 'T')\n",
        "    rna = rna.replace(\"T\", \"U\")\n",
        "\n",
        "    # Translate using the standard codon table, stop at first stop codon\n",
        "    protein = Seq(rna).translate(table=1, to_stop=True)\n",
        "    print(str(protein))\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    # Optional: pass a different path as the first argument\n",
        "    # main(sys.argv[1] if len(sys.argv) > 1 else \"rosalind_prot.txt\")\n",
        "    main() # Call main with the default path"
      ],
