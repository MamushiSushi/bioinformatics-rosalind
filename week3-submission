        "\n",
        "a = 4417\n",
        "b = 8848\n",
        "\n",
        "# Ensure the starting number is odd\n",
        "if a % 2 == 0:\n",
        "    start = a + 1\n",
        "else:\n",
        "    start = a\n",
        "\n",
        "total_sum = 0\n",
        "# Iterate through the odd numbers from start to b (inclusive) and sum them\n",
        "for number in range(start, b + 1, 2):\n",
        "    total_sum += number\n",
        "\n",
        "print(total_sum)"
