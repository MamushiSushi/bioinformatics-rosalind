        "#<font color=blue>**P23.**</font> SeqUtils.GC( ) – a function to compute GC content"
      ],
      "metadata": {
        "id": "SAjUYBpe3pwi"
      }
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "d4c0d7dd",
        "outputId": "e088dbd3-2db0-4bc2-b4c5-71abcdeb4e9a"
      },
      "source": [
        "# gc_content.py\n",
        "def parse_fasta(path):\n",
        "    records = {}\n",
        "    with open(path, \"r\") as f:\n",
        "        label = None\n",
        "        chunks = []\n",
        "        for line in f:\n",
        "            line = line.strip()\n",
        "            if not line:\n",
        "                continue\n",
        "            if line.startswith(\">\"):\n",
        "                if label is not None:\n",
        "                    records[label] = \"\".join(chunks)\n",
        "                label = line[1:].split()[0]  # e.g., \"Rosalind_0001\"\n",
        "                chunks = []\n",
        "            else:\n",
        "                chunks.append(line)\n",
        "        if label is not None:\n",
        "            records[label] = \"\".join(chunks)\n",
        "    return records\n",
        "\n",
        "def gc_percent(seq):\n",
        "    s = seq.upper()\n",
        "    gc = sum(1 for b in s if b in (\"G\", \"C\"))\n",
        "    return (gc / len(s)) * 100.0 if s else 0.0\n",
        "\n",
        "def main():\n",
        "    fasta = parse_fasta(\"rosalind_gc.txt\")\n",
        "    best_id, best_gc = max(((k, gc_percent(v)) for k, v in fasta.items()),\n",
        "                           key=lambda x: x[1])\n",
        "    print(best_id)\n",
        "    print(f\"{best_gc:.6f}\")  # six decimals works well for Rosalind\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()"
      ],
      "execution_count": 45,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Rosalind_1712\n",
            "51.338200\n"
          ]
        }
      ]
    },
